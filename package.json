{
  "name": "my-wiki",
  "version": "1.0.0",
  "description": "My custom wiki built with GitHub Wiki Framework",
  "type": "module",
  "scripts": {
    "predev": "node scripts/killDevServer.js && node scripts/copyConfig.js",
    "dev": "concurrently --kill-others --names \"CONFIG,NETLIFY\" --prefix-colors \"cyan,green\" \"npm:dev:watch\" \"npm:dev:server\"",
    "dev:server": "netlify dev",
    "dev:watch": "node scripts/watchConfig.js",
    "dev:vite": "concurrently --kill-others --names \"CONFIG,VITE\" --prefix-colors \"cyan,green\" \"npm:dev:watch\" \"vite\"",
    "prebuild": "node scripts/copyConfig.js && node scripts/buildSearchIndex.js",
    "build": "vite build",
    "preview": "vite preview",
    "build:search": "node scripts/buildSearchIndex.js",
    "validate:html": "node wiki-framework/scripts/validateHtml.js"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.2.3",
    "@uiw/react-codemirror": "^4.21.21",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "fuse.js": "^7.0.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.263.1",
    "octokit": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-highlight": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@octokit/rest": "^20.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "netlify-cli": "^17.0.0",
    "postcss": "^8.4.32",
    "sharp": "^0.33.0",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
